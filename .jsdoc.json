{
  "source": {
    "include": [
      "README.md"
    ],
    "exclude": [
      "node_modules",
      "docs",
      "browser"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "useLongnameInNav": false
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
